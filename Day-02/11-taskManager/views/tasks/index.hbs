<style>
    .completed {
        color : red;
        text-decoration : line-through;
        font-style : italic;
        font-weight : bold;
    }
</style>
<h1>{{title}}</h1>
<hr>
<ol>
    {{#each list}}
        {{#if this.isCompleted}}
            <li class='completed'>{{this.name}}</li>
        {{else}}
            <li>{{this.name}}</li>
        {{/if}}
    {{/each}}
</ol>
